{{#if result}}

  <div class="last-row">
    {{page-numbers content=result totalPages=80}}
  </div>

  <table class="table main">
    <tbody>
      {{#each result as |res|}}
        <tr class="last-row">
          <td>


<div class="list-group">
  <div class="list-group-item">
    <h4 class="list-group-item-heading">
      <span class="margin-10 pull-left">
        {{# if res.work }}{{ res.work }}{{ else }}<span class="underline-dotted work-disabled" title="Nombre no disponible">Nombre no disponible</span>{{/if}}<br>
      </span>
      {{#link-to "search" (query-params technique=res.technique) class="pull-right"}}{{ capitalize words=res.technique }}{{/link-to}}<br>
    </h4>
    <p class="list-group-item-text clear-both">
      {{# if res.artist0 }}Artista(s): {{ artists-involved exhibition=res }}{{/if}}<br>
      {{# if res.curator0 }}Curador(es): {{ curators-involved exhibition=res }}<br>{{/if}}
      {{#link-to "search" (query-params q=res.exhibition) class=""}}
        {{ res.exhibition }}
      {{/link-to}}, <span class="underline-dotted" title="{{ formatted-date date=res.start format='LL' }} al {{ formatted-date date=res.end format='LL' }}">{{ res.year }}</span>
      en {{#link-to "search" (query-params location=res.location)  class=""}}{{ res.location }}{{/link-to}}
    </p>
  </div>
</div>

          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>

  <div>
    {{page-numbers content=result totalPages=80}}
  </div>

  {{else}}

  <span class="text-big text-center"> ☝️</span> 

  {{/if}}

{{outlet}}
